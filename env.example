# Database Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Agent Model Configuration
LORE_AGENT_LLM=openai
LORE_AGENT_MODEL=gpt-4
STATBLOCK_AGENT_LLM=openai
STATBLOCK_AGENT_MODEL=gpt-4
CITATION_AGENT_LLM=openai
CITATION_AGENT_MODEL=gpt-4
ART_PROMPT_AGENT_LLM=openai
ART_PROMPT_AGENT_MODEL=gpt-4
PDF_AGENT_LLM=openai
PDF_AGENT_MODEL=gpt-4
QA_AGENT_LLM=openai
QA_AGENT_MODEL=gpt-4

# Iteration Model Configuration
ITERATION_1_LLM=openai
ITERATION_1_MODEL=gpt-4
ITERATION_2_LLM=openai
ITERATION_2_MODEL=gpt-4-turbo
ITERATION_3_LLM=openai
ITERATION_3_MODEL=gpt-3.5-turbo

# DALL-E Image Generation
DALLE_MODEL=dall-e-2
DALLE_SIZE=512x512
DALLE_QUALITY=standard
USE_DALLE_3=false

# Monster Generation Settings
GENERATION_COUNT=1
GENERATION_BATCH_SIZE=5
GENERATION_MAX_RETRIES=3
GENERATION_TIMEOUT_MS=30000

# Feature Flags
ENABLE_IMAGE_GENERATION=true
ENABLE_PDF_GENERATION=true
ENABLE_QA_REVIEW=true
ENABLE_ART_GENERATION=true

# Quality Settings
IMAGE_SIZE=512x512
STRICT_MODE=false

# Refinement Settings
REFINEMENT_ITERATIONS=3
FORCE_IMPROVEMENT=true

# Development Settings
DEBUG=false
LOG_LEVEL=info
MOCK_LLM=false
MOCK_IMAGE_GENERATION=false

# Node Environment
NODE_ENV=development 